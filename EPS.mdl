Changes in Capital Equipment Revenues and Associated Tax Revenues[Cash Flow Entity] :EXCEPT:\
		 [government],[nonenergy industries]=
	0 ~~|
Changes in Capital Equipment Revenues and Associated Tax Revenues[government]=
	(SUM(Amount Spent on Capital Equipment due to Process Emissions Policies[Industry Category\
		!,Industry Process Emissions Policy!])
	   + SUM(Amount Spent on Capital Equipment due to Energy Efficiency Policies[Industry Category\
		!,Industry Energy Efficiency Policy!])
	   + SUM(Industry Sector Change in CCS New Capital Eqpt Cost[Industry Category!])
	   + SUM(Amount Spent on Capital Equipment due to Industry Sector Change in CCS OM Cost\
		[Industry Category!])
	)
	* CESTR Capital Eqpt Sales Tax Rate ~~|
Changes in Capital Equipment Revenues and Associated Tax Revenues[nonenergy industries\
		]=
	(SUM(Amount Spent on Capital Equipment due to Process Emissions Policies[Industry Category\
		!,Industry Process Emissions Policy!])
	   + SUM(Amount Spent on Capital Equipment due to Energy Efficiency Policies[Industry Category\
		!,Industry Energy Efficiency Policy!])
	   + SUM(Industry Sector Change in CCS New Capital Eqpt Cost[Industry Category!])
	   + SUM(Amount Spent on Capital Equipment due to Industry Sector Change in CCS OM Cost\
		[Industry Category!])
	)
	* (1 - CESTR Capital Eqpt Sales Tax Rate)
	~	$
	~		|

Changes in Labor Revenues and Associated Tax Revenues[Cash Flow Entity] :EXCEPT: [government\
		],[labor and consumers]=
	0 ~~|
Changes in Labor Revenues and Associated Tax Revenues[government]=
	(SUM(Amount Spent on Labor due to Energy Efficiency Policies[Industry Category!,Industry Energy Efficiency Policy\
		!])
	  + SUM(Amount Spent on Labor due to Process Emissions Policies[Industry Category!,Industry Process Emissions Policy\
		!])
	  + SUM(Amount Spent on Labor due to Industry Sector Change in CCS OM Cost[Industry Category\
		!])
	)
	* WMITR Worker Marginal Income Tax Rate ~~|
Changes in Labor Revenues and Associated Tax Revenues[labor and consumers]=
	(SUM(Amount Spent on Labor due to Energy Efficiency Policies[Industry Category!,Industry Energy Efficiency Policy\
		!])
	  + SUM(Amount Spent on Labor due to Process Emissions Policies[Industry Category!,Industry Process Emissions Policy\
		!])
	  + SUM(Amount Spent on Labor due to Industry Sector Change in CCS OM Cost[Industry Category\
		!])
	)
	* (1 - WMITR Worker Marginal Income Tax Rate)
	~	$
	~		|

Changes in Nonenergy Revenue by Entity due to Production Changes[Cash Flow Entity] :EXCEPT:\
		  [nonenergy industries]=
	SUM(Change in Industry Expenditures on Nonenergy Inputs by Supplying Entity[Industry Category\
		!,Cash Flow Entity]) ~~|
Changes in Nonenergy Revenue by Entity due to Production Changes[nonenergy industries\
		]=
	SUM(Change in Industry Expenditures on Nonenergy Inputs by Supplying Entity[Industry Category\
		!,nonenergy industries])
	
	+ SUM(Change in Industry Revenues from Nonenergy Outputs by Purchasing Entity[Industry Category\
		!,Cash Flow Entity!])
	~	$
	~	Every cash flow entity receives revenue for their sales of nonenergy \
		inputs to all industries.  Additionally, nonenergy industries receive \
		revenue for their sales of nonenergy outputs to all cash flow entities.
	|

LULUCF Change in Nonenergy Expenditures by Entity[Cash Flow Entity]=
	Change in Forest Expenses by Entity[Cash Flow Entity]
	~	$
	~		|

LULUCF Change in Revenue by Entity[Cash Flow Entity] :EXCEPT:   [government],[nonenergy industries\
		],[labor and consumers]=
	0 ~~|
LULUCF Change in Revenue by Entity[government]=
	Change in Tax on Forest Equipment Expenses
	+ Change in Tax on Forest Labor Expenses ~~|
LULUCF Change in Revenue by Entity[nonenergy industries]=
	LULUCF Sector Change in Capital Eqpt Supplier Cash Flow ~~|
LULUCF Change in Revenue by Entity[labor and consumers]=
	LULUCF Sector Change in Labor Cash Flow
	~	$
	~		|

